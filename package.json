{
  "name": "cord-field",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:client && yarn build:ssr && yarn build:server",
    "build:client": "ng build --prod",
    "build:ssr": "ng build --prod --app 1 --output-hashing=false",
    "build:server": "webpack --config webpack.server.config.js --progress --colors",
    "coverage": "scripts/with-local-services.sh ng test --code-coverage --single-run; (open coverage/index.html || true)",
    "docker:build": "docker build --build-arg NG_BUILD_TARGET=local -t cord-field:latest .",
    "docker:build:tests": "docker build -f Dockerfile.test.dokr -t cord-field:tests .",
    "docker:verify": "scripts/docker:verify.sh",
    "e2e": "ng e2e",
    "lint": "tsc -p src/tsconfig.app.json --noEmit --pretty && tsc -p src/tsconfig.spec.json --noEmit --pretty && tsc -p e2e/tsconfig.e2e.json --noEmit --pretty && ng lint --format stylish",
    "serve:ssr": "node dist/server",
    "start": "scripts/with-local-services.sh ng serve --hmr --environment local",
    "start:manual": "ng serve --hmr --environment local",
    "start:production": "scripts/with-local-services.sh ng serve --hmr --environment prod",
    "test": "scripts/with-local-services.sh ng test --sourcemap",
    "test:bamboo": "scripts/test:bamboo.sh",
    "test:once": "yarn test --watch=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.5",
    "@angular/router": "^5.2.0",
    "@ncstate/sat-popover": "^1.0.0",
    "@nguniversal/common": "^5.0.0-beta.5",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "hammerjs": "^2.0.8",
    "localforage": "^1.6.0",
    "lodash.isequal": "^4.5.0",
    "rxjs": "^5.5.6",
    "ts-loader": "^3.5.0",
    "urlsafe-base64": "^1.0.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^9.4.6",
    "@types/urlsafe-base64": "^1.0.28",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "node-sass": "^4.9.0",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.6.2"
  }
}
