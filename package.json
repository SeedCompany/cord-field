{
  "name": "cord-field",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:client && yarn build:ssr && yarn build:server",
    "build:client": "ng build --prod",
    "build:ssr": "ng run cord-field:server",
    "build:server": "ts-node src/build-server",
    "coverage": "scripts/with-local-services.sh ng test --code-coverage --single-run; (open coverage/index.html || true)",
    "docker:build": "docker build --build-arg NG_BUILD_TARGET=local -t cord-field:latest .",
    "docker:build:tests": "scripts/docker:build:tests.sh",
    "docker:verify": "scripts/docker:verify.sh",
    "e2e": "ng e2e",
    "lint": "scripts/lint.sh",
    "serve:ssr": "node dist/server",
    "start": "scripts/with-local-services.sh ng serve -c local",
    "start:manual": "ng serve -c local",
    "start:production": "scripts/with-local-services.sh ng serve -c prod --hmr",
    "test": "scripts/with-local-services.sh ng test",
    "test:bamboo": "scripts/test:bamboo.sh",
    "test:once": "yarn test --watch=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.3",
    "@angular/cdk": "^7.2.3",
    "@angular/common": "^7.2.3",
    "@angular/compiler": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/forms": "^7.2.3",
    "@angular/http": "^7.2.3",
    "@angular/material": "^7.3.0",
    "@angular/platform-browser": "^7.2.3",
    "@angular/platform-browser-dynamic": "^7.2.3",
    "@angular/platform-server": "^7.2.3",
    "@angular/router": "^7.2.3",
    "@ncstate/sat-popover": "^3.1.0",
    "@nguniversal/common": "^7.1.0",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "@types/luxon": "^1.0.0",
    "@types/xregexp": "^3.0.29",
    "angulartics2": "^7.4.1",
    "core-js": "^2.4.1",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "libphonenumber-js": "^1.6.4",
    "localforage": "^1.6.0",
    "lodash-es": "^4.17.10",
    "luxon": "^1.2.1",
    "rxjs": "^6.4.0",
    "ts-loader": "^5.3.3",
    "typescript-lazy-get-decorator": "^1.2.2",
    "xregexp": "^4.2.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.3",
    "@angular/cli": "^7.2.3",
    "@angular/compiler-cli": "^7.2.3",
    "@angular/language-service": "^7.2.3",
    "@angularclass/hmr": "^2.1.3",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "^10.5.8",
    "@types/webpack": "^4.4.9",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "protractor": "^5.4.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "typescript": "~3.2.4"
  }
}
