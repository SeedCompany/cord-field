{
  "name": "cord-field",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:client && yarn build:ssr && yarn build:server",
    "build:client": "ng build --prod",
    "build:ssr": "ng run cord-field:server",
    "build:server": "ts-node src/build-server",
    "coverage": "scripts/with-local-services.sh ng test --code-coverage --single-run; (open coverage/index.html || true)",
    "docker:build": "docker build --build-arg NG_BUILD_TARGET=local -t cord-field:latest .",
    "docker:build:tests": "scripts/docker:build:tests.sh",
    "docker:verify": "scripts/docker:verify.sh",
    "e2e": "ng e2e",
    "lint": "scripts/lint.sh",
    "serve:ssr": "node dist/server",
    "start": "scripts/with-local-services.sh ng serve -c local",
    "start:manual": "ng serve -c local",
    "start:production": "scripts/with-local-services.sh ng serve -c prod --hmr",
    "test": "scripts/with-local-services.sh ng test",
    "test:bamboo": "scripts/test:bamboo.sh",
    "test:once": "yarn test --watch=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ncstate/sat-popover": "^2.1.1",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@types/luxon": "^1.0.0",
    "@types/xregexp": "^3.0.29",
    "core-js": "^2.4.1",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "localforage": "^1.6.0",
    "lodash-es": "^4.17.10",
    "luxon": "^1.2.1",
    "rxjs": "^6.1.0",
    "ts-loader": "^4.4.2",
    "typescript-lazy-get-decorator": "^1.2.2",
    "xregexp": "^4.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.3",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "^10.5.8",
    "@types/webpack": "^4.4.9",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "protractor": "^5.4.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "~2.9.0"
  }
}
